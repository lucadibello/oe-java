{
  "version": 3,
  "cmakeMinimumRequired": { "major": 3, "minor": 19 },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default build",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "OE_DEBUG_SIGN": "ON",
        "BUILD_JNI_HOST": "ON",
        "BUILD_HOST_EXE": "OFF"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "configurePreset": "default"
    }
  ]
}
